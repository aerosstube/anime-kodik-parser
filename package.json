{
  "name": "@aerosstube/anime-parser-kodik-ts",
  "version": "1.0.0",
  "description": "TypeScript библиотека для парсинга аниме с сайта Kodik с полной типизацией",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/entry.ts",
    "test": "npm run build && node dist/example.js",
    "example": "ts-node src/example.ts",
    "start": "ts-node src/entry.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "anime",
    "parser",
    "kodik",
    "m3u8",
    "streaming",
    "typescript",
